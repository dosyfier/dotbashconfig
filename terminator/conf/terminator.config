[global_config]
  always_split_with_profile = True
  enabled_plugins = TestPlugin, TerminatorThemes, TerminalShot, LaunchpadCodeURLHandler, APTURLHandler, LaunchpadBugURLHandler
  focus = system
  title_font = Hack 9
  title_inactive_bg_color = "#657b83"
  title_transmit_bg_color = "#dc322f"
  title_use_system_font = False
[keybindings]
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      profile = Nebula
      type = Terminal
    [[[window0]]]
      parent = ""
      profile = Nebula
      type = Window
[plugins]
[profiles]
  [[default]]
    allow_bold = False
    background_color = "#002b36"
    copy_on_selection = True
    cursor_color = "#93a1a1"
