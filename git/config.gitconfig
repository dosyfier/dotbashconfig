# vim: syntax=gitconfig:
[user]
	name = %DOTBASHCFG_USER%
	email = %DOTBASHCFG_MAIL%
[diff]
	tool = vimdiff
[merge]
	tool = vimdiff
[core]
	autocrlf = input
	editor = vim
[difftool]
	prompt = false
[push]
	default = simple
[pull]
	rebase = true
[credential]
	helper = cache
[mergetool]
	keepBackup = false
